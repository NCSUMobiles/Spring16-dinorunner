<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dino.ncsu.dinorunner">
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            android:configChanges="orientation|screenSize"
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".DinoPickerActivity"
            android:configChanges="orientation|screenSize" />
        <activity
            android:name=".TrackPicker"
            android:configChanges="orientation|screenSize"
            android:label="@string/title_activity_track_picker" />
        <activity
            android:name=".ItemPickActivity"
            android:configChanges="orientation|screenSize"
            android:label="Pick your items" />
        <activity
            android:name=".RunningActivity"
            android:configChanges="orientation|screenSize"
            android:label="@string/title_activity_running" />
        <!-- ATTENTION: This was auto-generated to add Google Play services to your project for
             App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information. -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
    </application>

</manifest>